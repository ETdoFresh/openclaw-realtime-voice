# OpenAI API Key (required for Realtime API)
OPENAI_API_KEY=

# Server port (optional, defaults to 3000)
PORT=3000

# OpenClaw Gateway Configuration
# Gateway WebSocket URL (defaults to wss://openclaw.etdofresh.com)
OPENCLAW_GATEWAY_URL=wss://openclaw.etdofresh.com

# Gateway authentication token (required for gateway connection)
OPENCLAW_GATEWAY_TOKEN=your_gateway_token_here

# Session key for conversation continuity (defaults to realtime-voice:ET)
OPENCLAW_SESSION_KEY=realtime-voice:ET
