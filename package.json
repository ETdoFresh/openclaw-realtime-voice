{
  "name": "openclaw-realtime-voice",
  "version": "1.0.0",
  "description": "Browser-based real-time voice interface for OpenClaw using OpenAI Realtime API",
  "type": "module",
  "main": "server/dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "NODE_ENV=production node server/dist/index.js",
    "preview": "vite preview"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "realtime",
    "voice",
    "openai",
    "webrtc",
    "voice-interface"
  ],
  "author": "ETdoFresh",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ETdoFresh/openclaw-realtime-voice"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "web-push": "^3.6.7",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
